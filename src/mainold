main() {
    let input = "false";
    let p = NbParser::parse(Rule::statement, &input)
        .expect("err")
        .next()
        .unwrap();
    let inner_rules = p.into_inner();
    println!("Input: \"{}\"\n\n{:#?}", input, inner_rules);

    println!("{}", SEPARATOR);

    let input2 = "t1 = true";
    let p2 = NbParser::parse(Rule::statement, &input2)
        .expect("err")
        .next()
        .unwrap();
    println!("Input: \"{}\"\n\n{:#?}", input2, p2);
}
